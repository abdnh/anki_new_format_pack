        "live-server": "^1.2.1",
        "prettier": "^2.4.1",
        "prettier-plugin-svelte": "^2.4.0",
        "sass": "^1.49.0",
        "svelte": "^3.43.2",
        "svelte-check": "^2.2.7",
        "svelte-preprocess": "^4.9.8",
        "typescript": "^4.4.4"
    }
